{
    "arrowParens": "always",
    "endOfLine": "lf",
    "importOrder": ["<THIRD_PARTY_MODULES>", "^[./]"],
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": ["classProperties", "decorators-legacy", "typescript", "jsx"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false
}
